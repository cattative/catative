{
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "devCommand": "npm run start",
    "routes": [
        {
            "src": "/assets/music/(.*)",
            "headers": {
                "Content-Type": "audio/mpeg",
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "continue": true
        },
        { "handle": "filesystem" },
        { "src": "/(.*)", "status": 404, "dest": "/404" }
    ]
}
